{
  "name": "console",
  "version": "0.1.4",
  "description": "A simple console editor",
  "author": "Firebox Technology",
  "hosts": {
    "editor-framework": "^0.5.0"
  },
  "pkgDependencies": {
    "ui-kit": "^0.1.0"
  },
  "main": "main.js",
  "main-menu": {
    "i18n:MAIN_MENU.panel.title/i18n:MAIN_MENU.panel.console": {
      "icon": "panel/icon.png",
      "accelerator": "CmdOrCtrl+0",
      "message": "console:open"
    }
  },
  "panels": {
    "panel": {
      "popable": false,
      "frame": "panel/console.html",
      "type": "dockable",
      "title": "i18n:MAIN_MENU.panel.console",
      "icon": "panel/icon.png",
      "height": 300,
      "shortcuts": {
        "command+k": "clear",
        "ctrl+k": "clear"
      },
      "messages": [
        "editor:console-log",
        "editor:console-success",
        "editor:console-failed",
        "editor:console-info",
        "editor:console-warn",
        "editor:console-error",
        "editor:console-clear"
      ]
    }
  }
}
